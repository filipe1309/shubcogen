#!/bin/bash

# Update Tag Version by commit

NEWEST_TAG=$(git describe --abbrev=0 --tags)

SCRIPT_VERSION=$(grep "^VERSION=" bin/deploy.sh | awk -F= '{print $2}')

echo $NEWEST_TAG
echo $SCRIPT_VERSION
echo "version $SCRIPT_VERSION"

