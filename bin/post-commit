#!/bin/bash

# DOTR

# Update Tag Version (with semantic versioning) on post-commit

NEWEST_TAG=$(git describe --abbrev=0 --tags)

SCRIPT_VERSION=$(head -n 1 bin/version)


echo $NEWEST_TAG
echo $SCRIPT_VERSION
echo "version $SCRIPT_VERSION"

